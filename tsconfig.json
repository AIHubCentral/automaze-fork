{
    "extends": "@tsconfig/node20/tsconfig.json",
    "compilerOptions": {
        "outDir": "./dist",
        "rootDir": "./src",
        "forceConsistentCasingInFileNames": true,
    },
    "include": [
        "src/**/*",
    ],
    "exclude": [
        "node_modules",
        "dist",
        "Archive",
        "src/Commands/",
        "src/CommandsContext/",
        "src/CommandsSlash/",
        "src/Core/",
        "src/Events/",
        "src/Utils/"
    ]
}